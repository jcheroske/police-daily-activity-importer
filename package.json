{
  "name": "police-daily-activity-importer",
  "description": "Imports police daily activity into a database.",
  "version": "0.0.1",
  "author": "Jay Cheroske <jay@portal.org>",
  "repository": {
    "type": "git",
    "url": "git://github.com/jcheroske/police-daily-activity-importer.git"
  },
  "bugs": {
    "url": "https://github.com/jcheroske/police-daily-activity-importer/issues"
  },
  "homepage": "https://github.com/jcheroske/police-daily-activity-importer#readme",
  "license": "MIT",
  "keywords": [
    "police",
    "daily",
    "activity",
    "racial",
    "justice"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run build:dev",
    "build:dev": "NODE_ENV=development webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack --progress --colors",
    "build:watch": "NODE_ENV=development webpack --progress --colors --watch",
    "clean": "rimraf dist/*",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "modules": "rimraf node_modules/* && npm i",
    "server": "pm2 start --attach ./ecosystem.config.js",
    "start": "npm run server",
    "stop": "pm2 stop all",
    "test": "ava --concurrency=5",
    "test:watch": "npm run test -- --watch"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@google/maps": "^0.3.1",
    "apollo-client": "^1.0.2",
    "bluebird": "^3.5.0",
    "envalid": "^2.4.2",
    "graphql-tag": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "node-cron": "^1.1.3",
    "pm2": "^2.4.4",
    "request-x-ray": "^0.1.4",
    "winston": "^2.3.1",
    "x-ray": "^2.3.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.1",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "webpack": "^2.3.3",
    "webpack-node-externals": "^1.5.4"
  },
  "devEngines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*{.,-}test.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "standard"
    ],
    "parser": "babel-eslint",
    "plugins": [],
    "root": true,
    "rules": {}
  }
}
